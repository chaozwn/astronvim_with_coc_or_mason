{
  "colors.enable": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "inlayHint.enable": false,
  "diagnostic.enableSign": true,
  "diagnostic.level": "hint",
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.errorSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "󰋼",
  "diagnostic.hintSign": "󰌵",
  "diagnostic.messageTarget": "echo",
  "diagnositc.displayByAle": true,
  "diagnostic.refreshOnInsertMode": false,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.virtualTextPrefix": "󰝤 ",
  "diagnostic.virtualText": true,
  "codeLens.enable": true,
  "list.previewHighlightGroup": "Statement",
  "list.nextKeymap": "<C-J>",
  "list.previousKeymap": "<C-K>",
  "snippets.ultisnips.directories": ["$HOME/.config/nvim/lua/user/coc_snippets/"],
  "suggest.noselect": false,
  "suggest.enablePreselect": true,
  "suggest.autoTrigger": "always",
  "suggest.timeout": 5000,
  "suggest.detailField": "preview",
  "suggest.triggerCompletionWait": 50,
  "suggest.completionItemKindLabels": {
    "method": " ",
    "function": " ",
    "variable": " ",
    "field": " ",
    "typeParameter": " ",
    "constant": " ",
    "class": " ",
    "interface": " ",
    "struct": " ",
    "event": " ",
    "operator": " ",
    "module": " ",
    "property": " ",
    "enum": " ",
    "reference": " ",
    "keyword": " ",
    "file": " ",
    "folder": " ",
    "color": " ",
    "unit": " ",
    "snippet": " ",
    "text": " ",
    "constructor": " ",
    "value": " ",
    "enumMember": " "
  },
  "pyright.enable": true,
  "python.formatting.provider": "yapf",
  "python.formatting.yapfArgs": [
    "--style",
    "{SPACES_AROUND_POWER_OPERATOR: True, SPACES_BEFORE_COMMENT: 1}"
  ],
  "python.pythonPath": "~/miniconda3/envs/python38/bin/python",
  "html.format.enable": true,
  "javascript.showUnused": true,
  "javascript.suggest.names": true,
  "javascript.suggestionActions.enabled": true,
  "prettier.printWidth": 100,
  "prettier.disableLanguages": [],
  "prettier.formatterPriority": 1,
  "prettier.useTabs": false,
  "prettier.trailingComma": "all",
  "prettier.singleQuote": false,
  "emmet.includeLanguages": {
    "vue-html": "html",
    "javascript": "javascriptreact"
  },
  "html.completion.attributeDefaultValue": "empty",
  "Lua.diagnostics.globals": ["vim"],
  "tsserver.enable": true,
  "tsserver.log": "normal",
  "tsserver.enableJavascript": true,
  "tsserver.useLocalTsdk": true,
  "typescript.inlayHints": true,
  "javascript.inlayHints": true,
  "typescript.suggestionActions.enabled": true,
  "typescript.format.enabled": true,
  "javascript.format.enabled": true,
  "typescript.suggest.autoImports": true,
  "tsserver.maxTsServerMemory": 8192,
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.referencesCodeLens.enabled": true,
  "typescript.referencesCodeLens.showOnAllFunctions": true,
  "typescript.preferences.includePackageJsonAutoImports": "on",
  "pairs.enableCharacters": ["(", "[", "{", "'", "\"", "`"],
  "pairs.enableBackspace": true,
  "css.lint.unknownAtRules": "ignore",
  "less.lint.unknownAtRules": "ignore",
  "scss.lint.unknownAtRules": "ignore"
}
